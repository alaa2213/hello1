import mongoose from "mongoose";

const AttendeeSchema = mongoose.Schema(                                
    {name:{
        type:String,
        required:true
    },
    age:{
        type:Number,
        required:true
    },
    email:{
        type:email
    }
}
)
const AttendeeModel = mongoose.model('Event', AttendeeSchemaSchema);

export default AttendeeModel;
